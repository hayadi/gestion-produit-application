<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Vente.
    -->
    <changeSet id="20190115102901-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="vente"
                                 constraintName="fk_vente_client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="commande_id"
                                 baseTableName="vente"
                                 constraintName="fk_vente_commande_id"
                                 referencedColumnNames="id"
                                 referencedTableName="commande"/>

        <addForeignKeyConstraint baseColumnNames="produit_id"
                                 baseTableName="vente"
                                 constraintName="fk_vente_produit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="produit"/>

    </changeSet>
</databaseChangeLog>
